---
import { Image } from '@astrojs/image/components';

const { logo, logo2 } = Astro.props;
---

<span
  class="w-8/12 flex items-center self-center ml-2 text-2xl md:text-xl font-bold text-gray-900 whitespace-nowrap dark:text-white"
>
  <Image class="hidden dark:block" src={logo} alt="logo" width={130} height={80} />
  <Image class="block dark:hidden" src={logo2} alt="logo2" width={130} height={80} />
</span>
